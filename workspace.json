{
  "version": 2,
  "projects": {
    "kasaobake": {
      "root": "apps/kasaobake",
      "sourceRoot": "apps/kasaobake",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/kasaobake",
            "outputPath": "dist/apps/kasaobake"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "kasaobake:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "kasaobake:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "kasaobake:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/kasaobake/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/kasaobake"
          ],
          "options": {
            "jestConfig": "apps/kasaobake/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "kasaobake-e2e": {
      "root": "apps/kasaobake-e2e",
      "sourceRoot": "apps/kasaobake-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/kasaobake-e2e/cypress.json",
            "tsConfig": "apps/kasaobake-e2e/tsconfig.e2e.json",
            "devServerTarget": "kasaobake:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kasaobake:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/kasaobake-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "kasaobake-ui": {
      "root": "libs/kasaobake/ui",
      "sourceRoot": "libs/kasaobake/ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/kasaobake/ui/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/kasaobake/ui"
          ],
          "options": {
            "jestConfig": "libs/kasaobake/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "kasaobake"
}