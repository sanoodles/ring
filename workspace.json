{
  "version": 2,
  "projects": {
    "iclab": {
      "root": "apps/iclab",
      "sourceRoot": "apps/iclab",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/iclab",
            "outputPath": "dist/apps/iclab"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "iclab:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "iclab:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "iclab:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/iclab/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/iclab"],
          "options": {
            "jestConfig": "apps/iclab/jest.config.js",
            "passWithNoTests": true
          }
        },
        "setup": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/iclab",
            "command": "node ./contentful/setup.js $CONTENTFUL_SPACE_ID $CONTENTFUL_MANAGEMENT_TOKEN"
          }
        },
        "types": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/iclab",
            "command": "contentful-typescript-codegen --output types/generated/contentful.d.ts"
          }
        }
      }
    },
    "iclab-ui": {
      "root": "libs/iclab/ui",
      "sourceRoot": "libs/iclab/ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/iclab/ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/iclab/ui"],
          "options": {
            "jestConfig": "libs/iclab/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "iclab"
}
